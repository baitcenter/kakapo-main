{
  "name": "client",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "react": "^16.5.2",
    "react-dom": "^16.5.2",
    "react-scripts": "2.0.5",
    "semantic-ui-css": "^2.4.1",
    "semantic-ui-react": "^0.83.0"
  },
  "scripts": {
    "start-fe": "cd client && react-scripts start",
    "start-be": "cargo run",
    "build": "cargo build --release && rm -rf ./www/ && cd client && react-scripts build && mv build ../www/",
    "test": "cargo test && cd client && react-scripts test",
    "eject": "cd client && react-scripts eject"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
